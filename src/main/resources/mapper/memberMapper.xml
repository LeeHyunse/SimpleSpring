<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper.memberMapper">

	<select id="selectMember" resultType="com.my.test.model.Member">
		SELECT
			*
		FROM
			member
	</select>
	
	<select id="selectMemberByName" parameterType="String" resultType="com.my.test.model.Member">
		SELECT
			*
		FROM
			member
		WHERE
			name=#{name}
		LIMIT 1
	</select>
	
	<insert id="insertMember" parameterType="com.my.test.model.Member">
		INSERT INTO member
			(name, age, register_ymdt)
		VALUES
			(#{name}, #{age}, NOW())
	</insert>
</mapper>